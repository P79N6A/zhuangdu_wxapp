.modal {
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0px;
    top: 0px;
    z-index: 99;
}

.musk {
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0px;
    top: 0px;
    background-color: rgba(0,0,0,0.5);
}

.show-form {
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0px;
    top: 0px;
}

.show-content {
    position: absolute;
    left: calc((100% - 600rpx)/2);
    top: 190rpx;
    width: 600rpx;
    height: 780rpx;
    border-radius: 20rpx;
    background: #fff;
}

.modal-bg {
    position: absolute;
    left: 0px;
    top: 0px;
    width: 600rpx;
    height: 780rpx;
    border-radius: 20rpx;
}

.modal-text {
    position: absolute;
    left: 50rpx;
    top: 182rpx;
}

.Agreement {
    position: absolute;
    left: 50rpx;
    top: 60rpx;
    font-weight: bold;
    font-size: 50rpx;
    color: #fff;
}

.ad-container {
    width: 700rpx;
    height: 225rpx;
    margin: 30rpx auto;
    z-index: 999;
}