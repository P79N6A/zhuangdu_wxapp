page {
    background: #f69;
    color: #fff;
}

.top {
    width: 100%;
    text-align: center;
    margin: 40rpx 0;
}

.top-title {
    font-size: 52rpx;
    font-weight: 600;
    letter-spacing: 4rpx;
}

.top-subtitle {
}

.box-content {
    width: 100%;
    position: relative;
    margin: 10rpx auto;
}

.box {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    margin: 20rpx;
    width: 94%;
    padding: 20rpx 0;
    background: #FF29B8;
    border-radius: 8rpx;
}

.own {
    width: 300rpx;
    display: felx;
    flex-direction: culumn;
    text-align: center;
    margin-right: 40rpx;
}

.own .own-img {
    width: 300rpx;
    height: 382rpx;
    border-radius: 8rpx;
}

.own text {
}

.star {
    width: 300rpx;
    display: felx;
    flex-direction: culumn;
    text-align: center;
}

.star .star-img {
    width: 300rpx;
    height: 382rpx;
    border-radius: 8rpx;
}

.upimg {
    width: 100%;
    text-align: center;
}

.upimg image {
    width: 300rpx;
    height: 77rpx;
}

.ad-container {
    width: 700rpx;
    height: 225rpx;
    margin: 30rpx auto;
    z-index: 999;
}

.group {
    overflow: hidden;
    font-size: 14px;
    white-space: nowrap;
    z-index: 99;
    margin: 60rpx 0;
}

.more {
    text-align: center;
    font-size: 40rpx;
    color: #000;
}

.tab-content {
    width: 90%;
    margin: 0 auto;
    background: #fff;
    padding: 10rpx;
    border-radius: 26rpx;
}

.icon-content {
    display: -webkit-flex;
    flex-direction: column;
    flex-wrap: wrap;
    text-align: center;
    margin: 10rpx 0;
    position: relative;
    border-radius: 10rpx;
    margin-right: 12rpx;
    overflow: hidden;
    display: inline-block;
    border: 2rpx solid #fff;
    box-sizing: border-box;
}

.icon image {
    margin: 10rpx auto;
    width: 120rpx;
    height: 120rpx;
    z-index: 1;
    display: inline-block;
    overflow: hidden;
    display: block;
}

.icon text {
    color: #666;
    font-size: 24rpx;
    line-height: 2;
    width: 100%;
    color: #343638;
    background-color: #fff;
    font-size: 20rpx;
    text-align: center;
    height: 40rpx;
    line-height: 40rpx;
    opacity: .9;
    z-index: 2;
    display: block;
}

.tab-content .models {
    padding: 18rpx 0;
}

.models {
}

.models .model {
    position: relative;
    width: 180rpx;
    height: 180rpx;
    border-radius: 10rpx;
    margin-right: 12rpx;
    overflow: hidden;
    display: inline-block;
    border: 2rpx solid #fff;
    box-sizing: border-box;
}

.models .model:first-child {
    margin-left: 30rpx;
}

.models .model.active {
    border-color: #0F30C3;
}

.models .model image {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 1;
}

.models .model .text {
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    color: #343638;
    background-color: #fff;
    font-size: 20rpx;
    text-align: center;
    height: 40rpx;
    line-height: 40rpx;
    opacity: .9;
    z-index: 2;
}

button {
    position: relative;
    display: block;
    margin: 0;
    padding: 0;
    box-sizing: border-box;
    text-align: center;
    text-decoration: none;
    line-height: 1;
    border-radius: 5px;
    -webkit-tap-highlight-color: transparent;
    overflow: hidden;
    color: #444;
    background-color: transparent;
}

button:after {
    border: none;
}

.button-hover {
    background: transparent;
    opacity: 1;
}

.save-content {
    width: 100%;
    display: felx;
    justify-content: center;
}

.save-content .sharebtn {
}

.shareBtn {
    width: 300rpx;
    height: 74rpx;
    margin: 0 auto;
}

.onloading {
    width: 100%;
    height: 100%;
    background: #fff;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 99;
    display: flex;
    justify-content: center;
    align-items: center;
}

.onloading image {
    width: 250rpx;
    height: 250rpx;
}