.container {
    width: 100%;
    padding: 20rpx 20rpx 0;
    height: 100vh;
    display: flex;
    flex-direction: column;
    align-items: center;
    background: #f5f5f5;
}

view {
    box-sizing: border-box;
}

.button-hover {
    background: transparent;
    opacity: 1;
}

.load_wrap {
    width: 750rpx;
    height: 100vh;
    background: #fff;
}

.load_block {
    position: fixed;
    top: 30%;
    left: 50%;
    transform: translateX(-50%);
    font-size: 30rpx;
    color: #333;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
}

.load_block image {
    width: 200rpx;
    height: 200rpx;
}

.hideText {
    animation: hide 1s;
}

.showText {
    animation: show 1s;
}

@-webkit-keyframes hide {
    0% {
        opacity: 1;
    }

    100% {
        opacity: 0;
    }
}

@keyframes hide {
    0% {
        opacity: 1;
    }

    100% {
        opacity: 0;
    }
}

@-webkit-keyframes show {
    0% {
        opacity: 0;
    }

    100% {
        opacity: 1;
    }
}

@keyframes show {
    0% {
        opacity: 0;
    }

    100% {
        opacity: 1;
    }
}

