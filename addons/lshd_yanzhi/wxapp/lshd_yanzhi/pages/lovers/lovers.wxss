.weui-grid {
    padding: 20px 10px;
    padding-bottom: 0;
}

.weui-grid {
    position: relative;
    float: left;
    padding: 20px 10px;
    width: 33.33333333%;
    box-sizing: border-box;
    border-right: 1rpx solid #d9d9d9;
    border-bottom: 1rpx solid #d9d9d9;
}

.avatar {
    width: 250rpx;
    height: 250rpx;
    border-radius: 50%;
}

.picker_wrap {
    align-items: center;
    display: flex;
}

.picker {
    font-size: 30rpx;
    color: #333;
}

.picker text {
    display: inline-block;
    line-height: 60rpx;
    border: 1rpx solid #eee;
    height: 60rpx;
    padding: 0 35rpx 0 10rpx;
    position: relative;
}

.picker text:after {
    content: '';
    display: block;
    width: 0;
    height: 0;
    border: 10rpx solid #333;
    position: absolute;
    top: 50%;
    margin-top: -5rpx;
    right: 10rpx;
    border-left: 10rpx solid transparent;
    border-bottom: 10rpx solid transparent;
    border-right: 10rpx solid transparent;
}

.age_btn {
    width: 680rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    color: #fff;
    font-size: 34rpx;
    background: #00c6c2;
    border-radius: 10rpx;
}

.age_btn.disabled {
    background: #7edddb;
}

.avatar_wrap {
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
}

.avatar_l,.avatar_r {
    position: relative;
    margin: 0 30rpx;
}

.avatar_wrap icon {
    position: absolute;
    bottom: 60rpx;
    right: 60rpx;
}

.tip_wrap {
    width: 750rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 30rpx;
    color: #333;
    margin-top: 40rpx;
