        opacity: 0;
        transform: scale3d(.2,.2,1);
    }

    to {
        opacity: 1;
        transform: scaleX(1);
    }
}

@keyframes c {
    0% {
        opacity: 0;
        transform: scale3d(.2,.2,1);
    }

    to {
        opacity: 1;
        transform: scaleX(1);
    }
}

@-webkit-keyframes d {
    0% {
        opacity: 1;
    }

    to {
        opacity: 0;
        transform: scale3d(.2,.2,1);
    }
}

@keyframes d {
    0% {
        opacity: 1;
    }

    to {
        opacity: 0;
        transform: scale3d(.2,.2,1);
    }
}

@-webkit-keyframes e {
    0% {
        opacity: .2;
    }

    to {
        opacity: 1;
    }
}

@keyframes e {
    0% {
        opacity: .2;
    }

    to {
        opacity: 1;
    }
}

@-webkit-keyframes f {
    0% {
        transform: scaleZ(0);
    }

    20% {
        transform: scale3d(1,1.1,0);
    }

    40% {
        transform: scale3d(1,.9,0);
    }

    60% {
        transform: scale3d(1,1.05,0);
    }

    80% {
        transform: scale3d(1,.95,0);
    }

    to {
        transform: scaleZ(0);
    }
}

@keyframes f {
    0% {
        transform: scaleZ(0);
    }

    20% {
        transform: scale3d(1,1.1,0);
    }

    40% {
        transform: scale3d(1,.9,0);
    }

    60% {
        transform: scale3d(1,1.05,0);
    }

    80% {
        transform: scale3d(1,.95,0);
    }

    to {
        transform: scaleZ(0);
    }
}

@-webkit-keyframes g {
    0% {
        opacity: 1;
    }

    to {
        opacity: 0;
    }
}

@keyframes g {
    0% {
        opacity: 1;
    }

    to {
        opacity: 0;
    }
}

.plaza-container {
    position: relative;
    height: 100%;
    background-color: #f5f5f5;
}

.plaza-container .pulldown-tips {
    width: 44rpx;
    height: 200rpx;
    top: 0;
    right: 32rpx;
    position: absolute;
    z-index: 10;
}

.plaza-container .scroll-wrapper {
    width: 100%;
    padding-bottom: 16rpx;
    background-color: #f5f5f5;
    padding-top: 16rpx;
    position: relative;
}

.plaza-container .scroll-wrapper .loading {
    width: 100%;
    height: 72rpx;
    display: block;
    vertical-align: middle;
    text-align: center;
    line-height: 72rpx;
}

.plaza-container .scroll-wrapper .loading .loadingstatus {
    color: #666;
    display: inline-block;
    margin-left: 16rpx;
    font-size: 24rpx;
}

.plaza-container .scroll-wrapper .loading .loadingicon {
    width: 24rpx;
    height: 24rpx;
    animation-timing-function: linear;
    animation-name: b;
    animation-duration: 1s;
    animation-iteration-count: infinite;
}

.plaza-container .scroll-wrapper .top-tips {
    background-color: #fff;
    height: 104rpx;
    width: 718rpx;
    border-radius: 6rpx;
    margin-left: 16rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.plaza-container .scroll-wrapper .top-tips .tips {
    font-size: 32rpx;
    color: #929292;
    letter-spacing: 0;
    line-height: 32rpx;
    height: 32rpx;
    vertical-align: middle;
    margin-left: 8rpx;
    margin-right: 8rpx;
}

.plaza-container .scroll-wrapper .top-tips .num {
    font-size: 32rpx;
    margin-left: 4rpx;
    height: 32rpx;
    vertical-align: center;
    text-align: center;
    line-height: 32rpx;
    color: #fd7352;
}

.plaza-container .scroll-wrapper .column-wrapper {
    width: 100%;
    display: flex;
}

.plaza-container .scroll-wrapper .column-wrapper .column {
    margin-left: 16rpx;
    display: inline-block;
}

.plaza-container .scroll-wrapper .column-wrapper .column .item {
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 351rpx;
    height: 520rpx;
    border-radius: 4rpx;
    margin-top: 16rpx;
    overflow: hidden;
    background-color: #fff;
    float: left;
    margin-right: 15rpx;
}

.plaza-container .scroll-wrapper .column-wrapper .column .item .footer {
    height: 151rpx;
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.plaza-container .scroll-wrapper .column-wrapper .column .item .footer .title {
    width: 307rpx;
    font-size: 32rpx;
    color: #242424;
    letter-spacing: -0.21rpx;
    line-height: 36rpx;
    margin-top: 28rpx;
    display: inline-block;
}

.plaza-container .scroll-wrapper .column-wrapper .column .item .footer .infos {
    display: flex;
    align-items: center;
    height: 100%;
    width: 307rpx;
    margin-top: 20rpx;
}

.plaza-container .scroll-wrapper .column-wrapper .column .item .footer .infos .avatar {
    width: 40rpx;
    height: 40rpx;
    border-radius: 20rpx;
}

.plaza-container .scroll-wrapper .column-wrapper .column .item .footer .infos .nickname {
    flex: 1;
    font-size: 28rpx;
    line-height: 40rpx;
    vertical-align: middle;
    margin-left: 8rpx;
    overflow: hidden;
    margin-right: 16rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #8f8f8f;
    display: inline-block;
}

.plaza-container .scroll-wrapper .column-wrapper .column .item .footer .infos .like-container {
    height: 100%;
    display: flex;
    align-items: center;
}

.plaza-container .scroll-wrapper .column-wrapper .column .item .footer .infos .like-container .likeicon {
    margin-right: 8rpx;
    width: 27rpx;
    height: 23rpx;
    display: inline-block;
}

.plaza-container .scroll-wrapper .column-wrapper .column .item .footer .infos .like-container .likes {
    margin-right: 0;
    font-size: 28rpx;
    color: #8f8f8f;
    letter-spacing: -0.18rpx;
    line-height: 39rpx;
    vertical-align: middle;
    display: inline-block;
}

.plaza-container .scroll-wrapper .column-wrapper .column .item .image-wrapper {
    width: 352rpx;
    height: 364rpx;
    position: relative;
    border-radius: 6rpx;
    overflow: hidden;
}

.plaza-container .scroll-wrapper .column-wrapper .column .item .image-wrapper .image {
    width: 100%;
    height: 100%;
    color: #e5e5e5;
}