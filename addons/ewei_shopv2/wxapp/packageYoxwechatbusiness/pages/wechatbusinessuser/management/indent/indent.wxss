@import "../../usertab_template/usertab_template.wxss";
.bgfff{
  background: #fff;
}
.nav{
  width: 100%;
  height: 56px;
  line-height: 60px;
  border-top: 1px solid #f5f5f5;
  border-bottom: 1px solid #f5f5f5;
}
.nav text{
  float: left;
  margin-left:62px;
}
.nav_active{
  color: #fd2d6b;
}
.nav_default{
  color: #929292;
}
.option{
  display: block;
  line-height: 38px;
  font-size: 30rpx;
  border-top: 1px solid #efefef;
  padding: 10rpx;
}
.optionBox{
  position: absolute;
  top:120rpx;
  width: 100%;
  border:1px solid #efefef;
  box-sizing: border-box;
  height: 0;
  overflow-y: auto;
  border-top: 0;
  background: #fff;
  transition: height 0.3s;
}
.addorder{
  position: fixed;
  left:266px;
  top:430px;
  width:70px;
  height:70px;
}
.addorder image{
  width: 100%;
  height: 100%;
}
.order_cell{
  width: 100%;
  height: 52px;
  background: #fff;
  border-bottom: 1px solid #ccc;
  padding-top:12px;
  padding-left:10px;
  font-size:14px;
}
.order_cell .order_id{
  margin-top:4px;
}
.order_cell .createtime{
   float: left;
}