.bgfff{
  background: #fff;
}
.date{
  width: 100%;
  height: 86px;
}
.date .date_title{
  width: 58px;
  font-size: 12px;
  color: #888;
  float: left;
  margin-left:14px;
}
.date .date_item{
  float: left;
  width: 66px;
  height: 25px;
  border-radius: 5px;
  margin-right: 6px;
  line-height: 26px;
  text-align: center;
  font-size: 12px;
  margin-bottom: 12px;
}
.date_active{
  border: 1px solid #fd2d6b;
  color: #fd2d6b;
}
.date_default{
  border: 1px solid #888;
  color: #888;
}
.dateline{
  width: 270px;
  height: 30px;
  background: #f5f5f5;
  line-height: 30px;
  clear: both;
  margin-left:72px;
  border-radius: 5px;
}
.dateline text{
  font-size: 14px;
  margin-left:24px;
}
.user_item{
  width: 125px;
  height: 32px;
  line-height: 32px;
  color: #acacac;
  font-size: 14px;
  float: left;
  text-align: center;
}
.user_active{
  background: #fd2d6b;
  color: #fff;
} 
.user_content{
}
.user_content_item{
  width: 100%;
  height: 512px;
  margin-top: 10px;
}
.user_title{
  margin-left: 20px;
  padding-top: 8px;
}