.bgfff{
  background: #fff;
}
.date{
  width: 100%;
  height: 86px;
}
.date .date_title{
  width: 52px;
  font-size: 12px;
  color: #888;
  float: left;
  margin-left:14px;
}
.date .date_item{
  float: left;
  width: 64px;
  height: 25px;
  border-radius: 5px;
  margin-right: 6px;
  line-height: 26px;
  text-align: center;
  font-size: 12px;
  margin-bottom: 12px;
}
.date_active{
  border: 1px solid #fd2d6b;
  color: #fd2d6b;
}
.date_default{
  border: 1px solid #888;
  color: #888;
}
.dateline{
  width: 270px;
  height: 30px;
  background: #f5f5f5;
  line-height: 30px;
  clear: both;
  margin-left:72px;
  border-radius: 5px;
}
.dateline text{
  font-size: 14px;
  margin-left:24px;
}
.canvasbox{
  width: 96%;
  height: 384px;
  border-radius: 5px;
  margin-top: 10px;
  margin-left:8px;
}
.canvasbox .sale_title{
  font-size: 20px;
  margin-left: 140px;
  padding-top:16px;
  margin-bottom:22px;
}
.sale_all{
  width: 154px;
  height: 135px;
  position: absolute;
  left: 134px;
  top: 228px;
  color: #fff;
}
.sale_all>text{
  font-size: 16px;
}
.sale_all text:last-child{
  font-size: 16px;
}
.single_conversion_rate{
  color: #fd2d6b;
  font-size: 20px;
  margin-left:26px;
  margin-top:30px;
}
.mar10{
  margin-top: 10px;
  width: 96%;
  border-radius: 5px;
  margin-left:8px;
  padding-top:21px;
}
.floatleft{
  float: left;
}
.mar8{
  margin-left: 8px;
}
.count{
  margin-top: 4px;
}
.p_count{
  margin-left: 24px;
  margin-right: 40px;
  margin-bottom:28px;
}
.cutline{
  height: 328px;
}
.cutline_title{
  margin-left:24px;
}
.order_amount{
  width: 148px;
  height: 60px;
  text-align: center;
  float: left;
  margin-left:20px;
}