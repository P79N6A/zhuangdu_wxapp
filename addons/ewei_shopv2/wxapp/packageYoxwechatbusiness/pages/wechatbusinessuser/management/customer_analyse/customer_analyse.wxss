.bgfff{
  background: #fff;
}
.date{
  width: 100%;
  height: 86px;
}
.date .date_title{
  width: 58px;
  font-size: 12px;
  color: #888;
  float: left;
  margin-left:14px;
}
.date .date_item{
  float: left;
  width: 66px;
  height: 25px;
  border-radius: 5px;
  margin-right: 6px;
  line-height: 26px;
  text-align: center;
  font-size: 12px;
  margin-bottom: 12px;
}
.date_active{
  border: 1px solid #fd2d6b;
  color: #fd2d6b;
}
.date_default{
  border: 1px solid #888;
  color: #888;
}
.dateline{
  width: 270px;
  height: 30px;
  background: #f5f5f5;
  line-height: 30px;
  clear: both;
  margin-left:72px;
  border-radius: 5px;
}
.dateline text{
  font-size: 14px;
  margin-left:24px;
}
.user_item{
  width: 125px;
  height: 32px;
  line-height: 32px;
  color: #acacac;
  font-size: 14px;
  float: left;
  text-align: center;
}
.user_active{
  background: #fd2d6b;
  color: #fff;
}
.bargainBox{
  margin-top: 10px;
  width: 96%;
  height: 488px;
  border-radius: 5px;
  margin-left:8px;
  padding-top:21px;
}
.new_amount{
  text-align: center;
  width: 94px;
  font-size: 18px;
  margin-left:126px;
  margin-top:100px;
  margin-bottom:100px;
}
.customer_amount{
  margin-left: 24px;
  margin-bottom: 16px;
}
.amount{
  font-size: 24px;
  margin-left: 18px;
}
.top_title{
  font-size: 20px;
  margin-left: 110px;
  padding-top:16px;
  margin-bottom:22px;
}