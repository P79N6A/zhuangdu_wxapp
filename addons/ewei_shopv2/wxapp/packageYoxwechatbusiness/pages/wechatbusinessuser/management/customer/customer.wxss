@import "../../usertab_template/usertab_template.wxss";
.bgfff{
  background: #fff;
}
.nav{
  width: 100%;
  height: 56px;
  line-height: 60px;
  border-top: 1px solid #f5f5f5;
  border-bottom: 1px solid #f5f5f5;
}
.nav text{
  float: left;
  margin-left:98px;
}
.nav_active{
  color: #fd2d6b;
}
.nav_default{
  color: #929292;
}
.option{
  display: block;
  line-height: 28px;
  font-size: 30rpx;
  border-top: 1px solid #efefef;
  padding: 10rpx;
}
.optionBox{
  position: absolute;
  top:120rpx;
  width: 100%;
  border:1px solid #efefef;
  box-sizing: border-box;
  height: 0;
  overflow-y: auto;
  border-top: 0;
  background: #fff;
  transition: height 0.3s;
}
.inviteuser_cell{
  width:110px;
  height: auto;
  border: 2px solid #ccc;
  text-align: center;
  float: left;
  background: #fff;
  font-size: 16px;
  margin-left: 20px;
}
.inviteuser_cell .avatar{
  width:75px;
  height:75px;
  margin-left:16px;
  margin-top:10px;
}
.inviteuser_cell .nickname{
  margin-top:6px;
}
.baseimg{
  width: 100%;
  height: 100%;
}
.tips{
  font-size: 16px;
  color: #c3c3c3;
  margin-left:126px;
  margin-top:216px;
}