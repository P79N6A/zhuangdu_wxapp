.tab{
  width: 100%;
  height: 50px;
  background: #fff;
  position: fixed;
  left: 0;
  bottom: 0;
  border-top:1px solid #ccc;
}
.tab .tab_cell{
  width: 50px;
  height: 45px;
  font-size: 12px;
  float: left;
  margin-left: 48px;
  margin-top: 8px;
  text-align: center;
  margin-right: 12px;
}
.tab .tab_cell .image{
  width: 22px;
  height: 22px;
  display: block;
}
.tab .tab_cell text{
  margin-top: 4px;
  display: block;
  margin-left:-26px;
}
.icon_active{
  background: #FD2D6B;
}
.icon_default{
  background: #727272;
}
.text_active{
  color: #FD2D6B;
}
.text_default{
  color: #727272;
}