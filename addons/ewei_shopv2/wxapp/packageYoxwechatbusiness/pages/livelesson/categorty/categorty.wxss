.categorty{
  width: 100%;
  height: auto;
  background: #fff;
  margin-bottom: 10px;
  float: left;
  padding-top:18px;
  padding-bottom:20px;
  padding-left:16px;
}
.categorty .title{
  font-size: 19px;
  margin-bottom:16px;
}
.categorty .item_cell{
  width: 82px;
  height: 40px;
  line-height: 40px;
  text-align: center;
  border: 1px solid #e2e2e2;
  border-collapse: collapse;
  float: left;
  color: #696969;
  font-size: 14px;
}