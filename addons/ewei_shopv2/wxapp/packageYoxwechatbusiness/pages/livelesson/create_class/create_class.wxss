.chose_title{
  width: 100%;
  height: 50px;
  line-height: 50px;
  margin-left: 10px;
}
.chose_list{
  width: 100%;
  height: 134px;
  background: #fff;
}
.chose_list .chose_cell{
  width: 66px;
  height: 96px;
  text-align: center;
  float: left;
  margin-left: 44px;
  margin-top:14px;
}
.chose_list .chose_cell .image{
  width: 42px;
  height: 44px;
  margin-left:10px;
}
.chose_list .chose_cell .image image{
  width: 100%; 
  height: 100%;
}
.chose_list .chose_cell .title{
  font-size: 14px;
}
.chose_list .chose_cell .choose{
  width:20px;
  height:20px;
  line-height:20px;
  color:#fff;
  border:1px solid #eae9e9;
  text-align:center;
  border-radius:20px;
  margin-left:20px;
  margin-top:14px;
}
.chose_list .chose_cell .choose.active{
  background: #fd2d6b;
}
.cate{
  width: 100%;
  height: 52px;
  line-height: 52px;
  background: #fff;
  margin-top: 8px;
}
.cate .cate_text{
  float: left;
  margin-left: 10px;
}
.cate .cate_icon{
  float: right;
  font-size: 24px;
  margin-right: 10px;
  color: #ccc;
}
.attent{
  width: 100%;
  height: 204px;
  color: #ccc;
  font-size: 14px;
}
.attent .up{
  width: 110px;
}
.attent  .image{
  float: left;
  width: 14px;
  height: 14px;
  line-height: 14px;
  text-align: center;
  border-radius: 14px;
  background: #eae9e9;
  color: #fff;  
}
.attent  .image.active{
  background: #008000;
}
.attent .up .attent_text{
  float: right;
}
.attent_attention,.attent_description,.attent_take{
  margin-left: 28px;
  margin-top: 24px;
}
.attent_attention.active,.attent_description.active,.attent_take.active{
  color: #008000;
}
.attent_detail{
  clear: both;
   font-size: 12px;
}