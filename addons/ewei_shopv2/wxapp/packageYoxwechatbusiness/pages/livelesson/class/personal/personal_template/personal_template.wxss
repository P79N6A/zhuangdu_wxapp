page{
  background: #fff;
}
.personal_cell{
  width: 100%;
  height: 116px;
  background: #fff;
  border-bottom:1px solid #f5f5f5;
  padding-top:14px;
}
.personal_cell .avatar{
  width: 46px;
  height: 46px;
  float: left;
  margin-left:14px;
}
.personal_cell .avatar image{
  width: 100%;
  height: 100%;
  border-radius: 46px;
}
.personal_cell .name{
  font-size: 13px;
  color: #8d8d8d;
  margin-left:72px;
  margin-bottom:4px;
}
.personal_cell .content{
  font-size: 15px;
  margin-left:72px;
}
.personal_cell .time{
  font-size:10px;
  color:#8d8d8d;
  float:right;
  margin-top:-26px;
  margin-right:24px;
}
.personal_cell .answer{
  width:56px;
  height:20px;
  line-height:20px;
  text-align:center;
  border:1px solid #fd2d6b;
  border-radius:10px;
  float:right;
  color:#fd2d6b;
  font-size:14px;
  margin-right:30px;
  margin-top:44px;
}