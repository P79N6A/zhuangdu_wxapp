page{
  background: #fff;
}
.text_input{
  width: 200px;
  height: 24px;
  border: 1px solid #ccc;
  float: left;
}
.send{
  width: 60px;
  height: 24px;
  line-height: 24px;
  text-align: center;
  border-radius: 10px;
  float: right;
  font-size: 16px;
  color: #fff;
  background: #fd2d6b;
  border: 1px solid #f5f5f5;
  margin-right:20px;
}
.bt{
  width: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 69;
  height: 100rpx;
}
 
.btTouch{
  background: #e2e2e2 !important;
  color: #333333 !important;
   width: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 69;
  height: 100rpx;
}