.time{
  width: 120px;
  height: 48px;
  color: #757474;
  margin-top:36px;
}
.time .time_left{
  font-size: 40px;
  float: left;
  margin-left:18px;
}
.time .time_right{
  float: right;
  font-size: 14px;
  margin-right:2px;
  margin-top:4px;
}
.nav{
  width: 100%;
  height: 82px;
  margin-top:374px;
}
.nav .nav_cell{
  width: 56px;
  float: left;
  height: 82px;
  margin-left:28px;
}
.nav .nav_cell .image{
  width: 48px;
  height: 48px;
  box-shadow: -0.1px -0.1px 1px #888;
