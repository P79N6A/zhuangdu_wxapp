.nav{
  width: 100%;
  height: 86px;
  padding-left:22px;
}
.nav .nav_cell{
  width: 46px;
  height: 28px;
  line-height: 28px;
  text-align: center;
  background: #f7f7f7;
  border-radius: 14px;
  float: left;
  font-size: 12px;
  margin-right: 10px;
  margin-top:10px;
}
.coupon_cell{
  width: 322px;
 box-shadow:-0.1px 0.1px 2px #888888;
  height: 98px;
  margin-left:20px;
  margin-top:18px;
}
.coupon_cell .coupon_cell_left{
  float: left;
  width: 248px;
  height: 98px;
}
.coupon_cell .coupon_cell_left .image{
  width: 94px;
  height: 76px;
  position: relative;
  margin-left:14px;
  margin-top:10px;
  float:left;
}
.coupon_cell .coupon_cell_left .image image{
  width: 100%;
  height: 100%;
}
.coupon_cell .content{
  width: 108px;
  height: 108px;
  float: left;
  font-size: 10px;
  margin-bottom: 4px;
  margin-left:10px;
  margin-top:16px;
}
.coupon_cell .content .content_text{
  font-size: 12px;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
}
.coupon_cell .content .price{
  font-size: 12px;
  color: #fd2d6b;
  margin-top: 6px;
} 
.coupon_cell .coupon_cell_right{
  float: right;
  color: #fd2d6b;
  margin-top:16px;
  margin-right:14px;
}
.coupon_cell .coupon_cell_right .coupon_price{

}
.coupon_cell .coupon_cell_right .use{
  width: 60px;
  height: 18px;
  color: #fff;
  font-size: 12px;
  text-align: center;
  line-height: 18px;
  background: #fd2d6b;
  border-radius: 18px;
}
.nav_cell.nav_active{
  color: #fd2d6b;
}