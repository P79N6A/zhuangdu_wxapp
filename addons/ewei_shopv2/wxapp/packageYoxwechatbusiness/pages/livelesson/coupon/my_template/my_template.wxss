.my_nav{
   width: 100%;
  height: 86px;
  padding-left:22px;
}
.my_nav .nav_cell{
  width:80px;
  height: 28px;
  line-height: 28px;
  text-align: center;
  background: #f7f7f7;
  border-radius: 14px;
  float: left;
  font-size: 12px;
  margin-right: 10px;
  margin-top:10px;
}
.my_cell{
  width: 322px;
  height: 98px;
  margin-left:20px;
  margin-top:18px;
  box-shadow:-0.1px 0.1px 2px #888888;
}
.my_cell .my_cell_left{
  float: left;
  width: 230px;
  height: 98px;
}
.my_cell .my_cell_left .content{
  width: 194px;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  margin-left:10px;
  margin-top:14px;
}
.my_cell .my_cell_left .day{
  color: #fd2d6b;
  font-size: 10px;
  margin-left:10px;
  margin-top:6px;
}
.my_cell .my_cell_right{
  float: right;
  width: 92px;
  height: 98px;
  background: #fd2d6b;
  color: #fff;
  text-align: center;
}
.my_cell .my_cell_right .price{
  font-size: 13px;
  margin-top:18px;
}
.my_cell .my_cell_right .use{
  font-size: 12px;
}