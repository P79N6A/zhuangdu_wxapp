.commit_cell{
  width: 100%;
  height: 90px;
  background: #fff;
  border-bottom: 1px solid #f5f5f5;
} 
.commit_cell .avatar{
  width: 40px;
  height: 40px;
  float: left;
  margin-left:28px;
  margin-top:14px;
}
 .commit_cell .avatar image{
  width: 100%;
  height: 100%;
  border-radius: 40px;
}
.commit_cell .commit_right{
  float: right;
  width: 284px;
  margin-right:10px;
}
.commit_cell .commit_right .name{
  font-size:15px;
  float: left;
  margin-top:12px;
}
.commit_cell .commit_right .name text{
  font-size: 11px;
  color: #c6d7e0;
}
.commit_cell .commit_right .like{
  float: right;
  width: 16px;
  height: 15px;
  margin-right:16px;
}
.commit_cell .commit_right .like image{
  width: 100%;
  height: 100%;
}
.commit_cell .commit_right .content{
  text-align: left;
  clear:both;
  width: 284px;
  margin-top:28px;
}