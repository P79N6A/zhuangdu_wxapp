.class_cell {
  width: 100%;
  height: 86px;
  border-bottom: 1px solid #f0f0f0;
  background: #fff;
}

.class_cell .image {
  width: 32px;
  height: 32px;
  float: left;
  margin-left:32px;
  margin-top:24px;
}

.class_cell .image image {
  width: 100%;
  height: 100%;
}
.class_cell .class_content{
  float: right;
}
.class_cell .class_content .title{
  width: 276px;
  font-size: 15px;
  margin-top:12px;
  margin-right:14px;
}
.class_cell .class_content .down{
  margin-top: 8px;
}
.class_cell .class_content .class_times{
  color: #e3e3e3;
  float: left;
  font-size:11px;
}
.class_cell .class_content .test{
  width: 34px;
  height: 15px;
  line-height: 15px;
  text-align: center;
  border: 1px solid #fd2d6b;
  border-radius: 8px;
  float: right;
  color: #fd2d6b;
  font-size: 11px;
  margin-right:14px;
}
.class_price{
  width: 306px;
  height: 36px;
  border-radius: 5px;
  background: #fd2d6b;
  color: #fff;
  line-height: 36px;
  text-align: center;
  font-size: 16px;
  margin-left:30px;
}