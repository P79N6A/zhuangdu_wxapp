.header{
    width:100%;
    background-color:#f3f3f3;
}
.my_name{
    height:70rpx;
    width:100%;
    line-height:70rpx;
    text-align:center;
}

.user_info{
    height:100rpx;
    width:100%;
    display:flex;
}

.avatar{
    width:170rpx;
    text-align:center;
    height:100%;
}

.nickname{
    width:400rpx;
    height:100%;
    display:flex;
     flex-direction: column;
}
.nickname text{
    height:50rpx;
    line-height:50rpx;
}
.edit{
    width:160rpx;
    height:100rpx;
    line-height:100rpx;
    color:#878484;
}


.goToUpgrade{
    width:100%;
    height:80rpx;
    line-height:80rpx;
    display:flex;

}
.goToUpgrade view{
    height:80rpx;
    display:flex;
    align-items:center;
     padding-left:35rpx;
}
.upgrade_button{
    color:white;
    background-color:#fd2d6b;
    width:110rpx;
    height:40rpx;
    text-align:center;
    padding:0;
    line-height:40rpx;
    font-size:23rpx;
}
.rank{
    color:#fd2d6b;
}
.creditmoney{
    width:100%;
    display:flex;
    justify-content:space-between;
    font-size:25rpx;
}
.credit,.money{
    height:120rpx;
    width:373rpx;
    text-align:center;
     background:#fff url(data:image/png;base64,) no-repeat;background-position:97% 93%;background-size: 15px 13px;
}
.credit view,.money view{
    height:60rpx;
    line-height:60rpx;
}
.creditmoney image{
    width:20rpx;
    height:20rpx;
    margin-right:10rpx;
}
.credit view:nth-child(1),.money view:nth-child(1){
    font-size:30rpx;
    line-height:80rpx;
}


.myOrder{
    margin-top:10rpx;
    width:100%;
    background-color:white;
}
.orderHeader{
    height:60rpx;
    line-height:60rpx;
    padding-left:40rpx;
    padding-right:40rpx;
    border-bottom:1rpx solid #ccc;
}
.orderHeader text:nth-child(1){
    float:left;
    font-weight:900;
    font-size:25rpx;
}
.orderHeader text:nth-child(2){
    float:right;
    color:#878484;
    font-size:25rpx;
}
.orderStatus{
    width:90%;
    margin-left:5%;
    margin-top:2rpx;
    display:flex;
    justify-content:space-between;
    height:140rpx;



}
.orderStatus view{
    display:flex;
     flex-direction: column;
    justify-content:center;
    text-align:center;
    align-items:center;
    font-size:23rpx;
    color:#878484;

}
.orderStatus image{
    width:58rpx;
    height:55rpx;
    margin-bottom:10rpx;
}
.detail{
    display:flex;
    flex-wrap: wrap;
    justify-content:space-between;

    background-color:#f3f3f3;
}
.detail_item{
    height:210rpx;
    background-color:white;
    width:24%;
    text-align:center;
    margin-top:10rpx;
    display:flex;
     flex-direction: column;
    justify-content:center;
    align-items:center;
    font-size:25rpx;
    color:#878484;
}
.detail_item image{
    margin-bottom:15rpx;
}

.more{
    margin-top:10rpx;
    height:170rpx;
    width:100%;
    background-color:white;
    text-align:center;
    display:flex;
    justify-content:center;
    align-items:center;
}

.more button{
    width:330rpx;
    height:60rpx;
    border-radius:20rpx;
    border:3rpx solid black;
    background-color:#fd2c6b;
    color:white;
    line-height:60rpx;
    font-size:28rpx;
}