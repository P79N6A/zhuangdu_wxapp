
.header{
  height:190rpx;
  width:100%;
  background:white;
   border-top:1px solid #f5f5f5;
   position:fixed;
   top:0;
   left:0;
   z-index:100;
}
.search{
  padding-top:20rpx;

  width:100%;
  height:75rpx;
     display: flex;
   flex-direction: row;
    justify-content:center;
}
.search input{
  width:94%;
  border:none;
  border-radius:30px;
  height: 56rpx;
  line-height: 56rpx;
  color:#666;
  font-size:22rpx;
  text-align:center;
  background-color:#f5f5f5;

}
.search-icon{
    background:#f5f5f5 url(data:image/png;base64,) no-repeat;background-size: 15px 15px;
      background-position:43% 50%;
}

.nav{
  height:115rpx;
  width:100%;
  line-height:85rpx;
   display: flex;
   flex-direction: row;
  font-size:22rpx;
   justify-content: center;
}

.nav view{
  text-align:center;
  width:150rpx;
}

.choose{
  color:#fd2d6b;
  border-bottom: 1px solid #fd2d6b;
  padding-bottom: 10rpx;
}

.nolist{
    width:100%;
    height:100%;
    color:#fd2d6b;
    display: flex;
    align-items:center;
   justify-content: center;
   position:absolute;
   top:0;
   left:0;

}


.list{
  margin-top:210rpx;
  width:100%;
}
.item{
    height:70rpx;
    width:90%;
    display: flex;
    align-items:center;
    justify-content:space-between;
    background:white;
    font-size:26rpx;
    padding-left:5%;
    padding-right:5%;
    margin-bottom:8rpx;
}
.alive{
  color:#fd2d6b;
}
.yaoqing{
    position:fixed;
    bottom:10%;
    right:5%;
    width:198rpx;
    height:86rpx;
}

