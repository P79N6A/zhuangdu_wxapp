.header{
  height:275rpx;
  width:100%;
  background:white;
   border-top:1px solid #f5f5f5;

}
.little_title{
    display: flex;
    align-items:center;
    justify-content: center;
    height:100rpx;
    width:100%;
}

.count{
  width:92%;
  margin-left:4%;
  display: flex;
  height:100rpx;
   flex-direction: row;
    justify-content:center;
}

.type{
  height:100%;
  text-align:center;
  margin-right:20rpx;
}
.type_num{
  color:#fd2d6b;
}
.type_name{
  font-size:24rpx;
}


.time{
  height:60rpx;
  width:40%;
  margin-left:30%;
  display: flex;
  flex-direction: row;
  justify-content:space-between;
  font-size:24rpx;
    align-items:center;

}
.prev,.next{
  text-align:center;
  line-height:40rpx;
  color:white;
  width:40rpx;
  height:40rpx;
  border-radius:50%;
  background:#fd2d6b;
}


.list{
  width:100%;
  margin-bottom:200rpx;
}
.item{
    height:100rpx;
    width:100%;
    display: flex;
    flex-direction: column;
    background:white;
    font-size:29rpx;
    padding-left:5%;
    margin-top:22rpx;
    padding-top:20rpx;


}
.price,.list_type_name{
  color:#fd2d6b;
}

.bottom{
  color:white;
  width:100%;
  height:100rpx;
  background:#fd2d6b;
  position: fixed;
  bottom:0;
  left: 0;
  display: flex;
    align-items:center;
  justify-content:center;


}
.bottom image{
  width:38rpx;
  height:38rpx;
  margin-right:20rpx;
}


.shadow{
  width:100%;
  height:100%;
  position:fixed;
  top: 0;
  left: 0;
  background:rgba(0, 0, 0, 0.4);
  opacity:0.6;
  z-index:101;
  display:flex;
  align-items:center;
  justify-content:center;

}
.share_block{
  height:470rpx;
  width:478rpx;
  background-color:white;
  z-index:1000;
  border-radius:20rpx;
  position:absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.share_img{
  width:100%;
  height:355rpx;
}

.share_button{
  width:100%;
  height:115rpx;
  display:flex;
   align-items:center;
  justify-content:space-between;

}

.share_button button{
  height:50rpx;
  background-color:#fd2d6b;
  color:white;
  border-radius:20rpx;
  font-size:22rpx;
  line-height:50rpx;
}

.choose{
  background:#fff url(data:image/png;base64,) no-repeat;background-position:90% 50%;background-size: 30px 25px;
}

.rule{
    position:absolute;
    right:5rpx;
    top: 25rpx;
    width:142rpx;
    height:50rpx;
    background-color:#fd2d6b;
    color:white;
    font-size:21rpx;
    line-height:50rpx;
    text-align:center;
}
