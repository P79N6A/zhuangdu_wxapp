.banner{
  height: 400px;
  background-color:#ddd;
}
.bgfff{
  background: #fff;
}
.customSwiper {
  height: 420rpx;
  position: relative;
}
.customSwiper swiper{
  height: 100%;
}
.tab{
  height: 7orpx;
  position: absolute;
  bottom: 6px;
  display:flex;
  width: 100%;
  text-align: center;
  justify-content: center;
  align-items: center;
}
.tab .tab-item{ 
  background-color: #ccc;
  height: 20rpx;
  width: 20rpx;
  line-height: 50rpx;
  font-size:12rpx;
  color:#fff;
  border-radius: 12rpx;
  margin-right:10px;
}
.tab .tab-item.selected{ 
  background-color: #fd2c6b;
}
.detail-tab{
  height: 80rpx;
  width: 100%; 
  box-sizing: border-box;
  overflow: hidden;
  line-height: 80rpx;
  background: #fff; 
  font-size: 16px; 
  white-space: nowrap;
  border-bottom: 1px solid #f1f1f1;
}
.detail-tab .detail-tab-item{
  margin:0 32px;
  display: inline-block;
}
.detail-tab .detail-tab-item.active{
  position: relative;
  color: #000;
}
.detail-tab .detail-tab-item.active::after{ 
  content: "";
  display: block;
  height: 8rpx;
  width: 60px;
  background: #fd2c6b;
  position: absolute; 
  bottom: 0;
  left: 0;
  border-radius: 16rpx;
}
.detail_main{
  width: 100%;
  min-height: 40px;
  padding-top: 10px;
}
.detail_main .detail_main_cell{
  font-size: 36rpx;
  margin-left: 10px;
  margin-top:10px; 
}
.detail_content{
  width: 100%;
  height: 103px;
  border-bottom: 1px solid #ccc;
}
.detail_content .image{
  width: 62px;
  height: 62px;
  margin-left:24px;
  margin-top:20px;
  float: left;
}
.baseimg{
  width: 100%;
  height: 100%;
}
.detail_content .detail_content_left{
  float: left;
}
.detail_content .detail_content_right{
  float:right;
  margin-right:18px;
  width:68px;
  margin-top:56px;
}
.detail_content .main{
  float:left;
  margin-top:30px;
  margin-left:16px;
}
.detail_content .main .title{
  font-size: 30rpx;
}
.detail_content .main .use{
  font-size: 20rpx;
  color: #bdbdbd;
  margin-top:8px;
}
.detail_content .detail_content_right .place{
  font-size:22rpx;
  display:block;
  margin-left:17px;
  color: #bdbdbd;
}
.content_down{
  width: 100%;
  height: 32px;
  margin-bottom: 10px;
}
.content_down .content_down_left{
  float: left;
  height: 32px;
  margin-left:22px;
  margin-top:8px;
}
.content_down .content_down_left text{
  font-size: 24rpx;
}
.content_down .content_down_left .price{
  color: #fd2c6b;
  margin-left:12px;
}
.content_down .content_down_right{
  float: right;
  margin-right:12px;
  margin-top:8px;
}
.content_down .content_down_right image{
  width: 15px;
  height: 14px;
  margin-right:4px;
}
.content_down .content_down_right text{
  font-size: 24rpx;
  color: #bcbcbc;
}
.fix_buy{
  width: 100%;
  border-top: 1px solid #f0f0f0;
  height: 53px;
  position: fixed;
  left: 0;
  bottom: 0;
}
.fix_buy .shop{
  width: 60px;
  float: left;
  margin-top:8px;
  margin-left:20px;
}
.fix_buy .shop .image,.fix_buy .collect .image{
  width: 20px;
  height: 18px;
}
.fix_buy .shop text,.fix_buy .collect text{
  display: block;
  font-size: 24rpx;
  color: #c7c7c7;
  margin-left: -2px;
}
.fix_buy .collect{
  width: 28px;
  float: left;
  margin-top:8px;
  margin-left:20px;
}
.fix_buy .buy_btn{
  width: 242px;
  height: 53px;
  line-height: 53px;
  text-align: center;
  background: #fd2c6b;
  color: #fff;
  font-size: 30rpx;
  float: right;
}
.fix_buy .buy_btn text{

}
.show_more{
  width: 100%;
  font-size: 32rpx;
  color: #bdbdbd;
  text-align: center;
  margin-bottom: 10px;
}
.show_more image{
  width: 18px;
  height: 8px;
}
.detail_commend{
  width: 100%;
  height:400px;
}
.detail_commend .detail_commend_cell{
  float: left;
  width: 100px;
  height: 140px;
  margin-left:16px;
  margin-top:10px;
}
.detail_commend .detail_commend_title{
  margin-left: 20px;
  font-size: 32rpx;
  padding-top:6px;
}
.detail_commend .detail_commend_cell .image{
  width: 100px;
  height: 100px;
}
.detail_commend .detail_commend_cell .title{
  font-size:28rpx;
  margin-left:8px;
}
.detail_commend .detail_commend_cell .price{
  font-size: 28rpx;
  color: #fd2c6b;
  margin-left:8px;
}