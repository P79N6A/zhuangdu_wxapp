.userInfo{
  width: 100%;
  height: 120px;
  background: #fd2c6b;
}
.bgfff{
  background: #fff;
}
.userLeft{
  float: left;
}
.userLeft .avatar{
  height:160rpx;
  width:160rpx;
  float:left;
  margin-right:18px;
}
.userLeft .avatar image{
  height:160rpx;
  width:160rpx;
  border-radius:160rpx;
  float:left;
  position:relative;
  display:-webkit-box;
  display:-webkit-flex;
  flex-shrink:0;
  -webkit-box-lines:single;
  -moz-box-lines:single;
  flex-wrap:nowrap;
  box-sizing:border-box;
  -webkit-box-align:center;
  align-items:center;
  margin-left:20px;
  margin-top:20px;
}
.nicknameBox{
  height:auto;
  position:relative;
  /* width:100%; */
  overflow:hidden;
  box-sizing:border-box;
  display:block;
  padding-left:20rpx;
  color:#fff;
  margin-top:18px;
}
.nicknameBox .name{
  font-size:36rpx;
  line-height:40rpx;
}
.nicknameBox .level{
  font-size:28rpx;
  line-height:36rpx;
}
.nicknameBox .credit{
  font-size:28rpx;
  line-height:36rpx;
}
.nicknameBox .credit text{
  color:#fef31f;
}
.userRight{
  float: right;
  width:230rpx;
  height:72rpx;
  margin-top:40px;
}
.baseimg{
  width: 100%;
  height: 100%;
}
.menu-group{
  width: 100%;
  height: 40px;
}
.menu-group .menu-item{
  padding:20rpx;
  text-align:center;
  position:relative;
  color:#666;
  font-size:30rpx;
  width:1%;
  display:table-cell;
  -webkit-user-select:none;
  -moz-user-select:none;
  transition:background-color 300ms;
}
.cell-group{
  width: 100%;
  height: 42px;
  line-height: 42px;
  font-size: 14px;
}
.cell-group .cell{
  position:relative;
  /* padding:24rpx; */
  display:-webkit-box;
  display:-webkit-flex;
  display:-ms-flexbox;
  display:flex;
  -webkit-box-align:center;
  -ms-flex-align:center;
  align-items:center;
  transition-duration:300ms;
  transition-property:background-color;
  padding-left:14px;
}
.cell-group .cell-icon{
  color: #999;
  margin-right: 20rpx;
  width:32rpx;
  height:32rpx;
  line-height:32rpx;
  text-align:center;
  /* margin-bottom:9px; */
}
.cell-group .cell-icon .icox-shop{
  font-size: 38rpx;
}
.cell-group .cell-text{
  color: #000;
  line-height: 1;
  /* margin-right:218px; */
  width:262px;
  /* margin-bottom:9px; */
}
.cell-group .cell-remark{
  color:#999;
  text-align:right;
  font-size:26rpx;
  margin-right:8rpx;
}
.group{
  position: relative;
  width: 100%;
  height: 88px;
}
.icon-col{
  width: 20%;
  height:auto;
  position:relative;
  padding:0;
  margin:10px 0;
  text-align:center;
  transition:background-color 300ms;
  float:left;
  border:none!important;
}
.icon-col .icon{
  height:88rpx;
  margin:auto;
  text-align:center;
  line-height:88rpx;
}
.icon-col .icon image{
  width: 88rpx;
  height: 88rpx;
}
.icon-col .text{
  color: #666;
  font-size:24rpx;
  text-align:center;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  padding:8rpx;
  width:100%;
  box-sizing:border-box;
}
.icon2-group{
  width: 100%;
  height: 262px;
}
.marginbottom{
  margin-bottom: 8px;
}
.picture{
  width: 100%;
  height: 50px;
}
.picture .picture-main{
  width: 278px;
  height: 35px;
  padding: 8px 46px;
}
.picture image{
  height: 33.8269px;
  width: 100%;
}
.copyright2-group{
  width: 100%;
  height: 35px;
  text-align:center;
  padding:10rpx 0;
}
.copyright2-group .copyright_cover{
  font-size: 12px;
  line-height: 30px;
}
.right-icon{
  width: 24px;
  height: 24px;
  margin-bottom:20px;
}