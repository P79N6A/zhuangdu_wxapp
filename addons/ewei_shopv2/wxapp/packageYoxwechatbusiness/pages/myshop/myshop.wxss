/* packageYoxwechatbusiness/pages/myshop/myshop.wxss */
.userBox{
  width: 100%;
  height: 400rpx;
  background: #f52462;
  text-align:center;
}
.userInfo{
  width:100%;
  font-size:42rpx;
  color:#f0f0f0;
}
.userinfo-avatar{
  width: 172rpx;
  height: 172rpx;
  border-radius: 86rpx;
  margin-left:38%;
  padding-top: 32px;
}
.userinfo-nickname{
  margin-top:6px;
}
.menu{
  margin-top: 20px;
  width: 100%;
}
.menu-cell{
  width: 33%;
  height: 200rpx;
  border-top: 1px solid #f0f0f0; 
  border-right: 1px solid #f0f0f0;
  border-bottom: 1px solid #f0f0f0;
  float: left;
  background: #fff;
  padding-top:40rpx;
  border-collapse:collapse; 
  font-size: 12px;
  text-align: center;
}
/* .menu-cell:nth-child(2n+1){
  border-collapse:collapse; 
} */
.menu-cell image{
  margin-left:61rpx;
  margin-right:58rpx;
}
.menu-cell text{
  margin-top:16rpx;
  display:block;
}
.collect{
  width:68px;
  height:24px;
  border:1px solid #ccc;
  color:#f5f5f5;
  line-height:24px;
  text-align:center;
  border-radius:60px;
  margin-left:148px;
  font-size:14px;
  margin-top:6px;
}