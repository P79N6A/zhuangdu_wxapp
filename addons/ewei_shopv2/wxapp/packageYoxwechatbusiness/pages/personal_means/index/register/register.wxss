.register_box{
  width: 358px;
  height: 342px;
  border-radius: 5px;
}
.register_box .title{
  width: 358px;
  height: 86px;
  line-height: 86px;
  text-align: center;
  background: #fd2d6b;
  color: #fff;
  font-size: 25px;
  position: relative;
}
.register_box .title text{

}
.register_content{
  width: 358px;
  height: 254px;
}
.register_content .register_content_phone{
  width: 288px;
  margin-left:30px;
  margin-top:20px;
}
.register_content .register_content_phone .phone_input{
  width: 288px;
  height: 45px;
  border: 1px solid #fd2d6b;
  border-radius: 5px;
}
.register_content_input{

}
.register_content .register_content_gain{
  width: 288px;
  margin-top:14px;
  margin-left:30px;
  margin-bottom:84px;
}
.register_content .register_content_gain .check{
  width: 164px;
  height: 45px;
  border: 1px solid #fd2d6b;
  border-radius: 5px;
  float: left;
}
.register_content .register_content_gain button{
  width: 112px;
  height: 45px;
  line-height: 45px;
  text-align: center;
  float: right;
  color: #fff;
  border-radius: 5px;
  background: #fd2d6b;
}
.register_content_policy{
  width: 210px;
  height: 16px;
  clear: both;
  margin-left:76px;
  margin-bottom:22px;
}
.register_content_policy text{
  font-size: 12px;
}
.register_button{
  width: 232px;
  height: 44px;
  line-height: 44px;
  text-align: center;
  border-radius: 20px;  
  background: #fd2d6b;
  color: #fff;
}