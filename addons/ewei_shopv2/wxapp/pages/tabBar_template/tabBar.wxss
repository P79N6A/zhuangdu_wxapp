.nav-tabs {
  width: 100%;
  display: flex;
  position: fixed;
  bottom: 0;
  border-top:1px solid #d3d3d6; 
}

.tab-list {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column-reverse;
  background: #f7f7fa;
  height:92rpx;
}

.tab-text {
  font-size: 24rpx;
  line-height: 35rpx;
  color: #999999;
  margin-left: -1px;
  margin-top: 16rpx;
  display: block;
}

.iconPath {
  width:100%;
  height: 100%;
  /* margin-top: 8rpx; */
  float: left;
}

.tab-list:nth-child(3) image,.tab-list:nth-child(5) image{
  margin-left: 4px;
}


.tab-list:nth-child(3) text{
  margin-left:3px;
}

.tab-content {
  flex: 1;
}

.tabBar-default {
  /* line-height: 75rpx; */
  line-height: 4rpx !important;
  text-align: center;
  flex: 1;
  color: #999999;
  /* font-weight: bold; */
  font-size: 21rpx;
}

.tabBar-active {
  /* line-height: 75rpx; */
  line-height: 4rpx !important;
  text-align: center;
  color: #fd2d6b;
  flex: 1;
  /* font-weight: bold; */
  font-size: 21rpx;
}

.show {
  display: block;
  flex: 1;
}

.hidden {
  display: none;
  flex: 1;
}
.tabimage{
  width: 56rpx;
  height: 52rpx;
}