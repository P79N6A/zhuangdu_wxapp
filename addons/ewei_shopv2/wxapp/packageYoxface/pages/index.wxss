.picture_btn{
  width: 300px;
  height: 300px;
  border-radius: 300px;
  background: #fff;
  margin-left:34px;
}
.picture_btn .image{
  width: 48px;
  height: 48px;
  margin-left:128px;
margin-top:114px;
padding-top:50px;
}
.picture_btn .image image{
  width: 100%;
  height: 100%;
  border-radius:48px;
}
#url{
  width: 200px;
  height: 200px;
  margin-left:-78px;
  margin-top:-26px;
}
.picture_btn .text{
  font-size: 16px;
  width:210px;
  height: 16px;
  margin-left:54px;
  margin-top:26px;
}
.picture_btn .btn{
  width: 158px;
  height: 46px;
  line-height: 46px;
  background: #fd2c6b;
  border-radius: 100px;
  font-size: 28px;
  color: #fff;
  margin-left:76px;
  margin-top:135px;
}
.result_image{
  width: 500px;
  height: 500px;
  z-index: -1;
}
.result_image image{
  width: 100%;
  height: 100%;
}
.check{
  width: 158px;
  height: 46px;
  line-height: 46px;
  text-align: center;
  border-radius: 200px;
  background: #fd2c6b;
  color: #fff;
  font-size: 22px;
  margin-left:68px;
  margin-top:56px;
}