.hotsearch_cell{
  width: 100%;
  height: 68px;
  line-height: 68px;
  background: #fff;
  border-bottom: 2px solid #e4e4e4;
}
.hotsearch_cell .hotsearch_cell_left{
  float: left;
}
.hotsearch_cell .number{
  width: 22px;
  height: 22px;
  line-height: 23px;
  text-align: center;
  border-radius: 11px;
  /* background: #ccc; */
  float: left;
  color: #646464;
  margin-left:12px;
  margin-top:23px;
  margin-right:16px;
}
.hotsearch_cell:nth-child(1) .number{
  background: #ffc627;
  font-size:26rpx;
  color: #fff;
}
.hotsearch_cell:nth-child(2) .number{
  background: #d2d2d2;
  font-size:26rpx;
  color: #fff;
}
.hotsearch_cell:nth-child(3) .number{
  background: #f9dbdb;
  font-size:26rpx;
  color: #fff;
  opacity: 0.8;
}
.hotsearch_cell .title{
  font-size: 38rpx;
  color: #050505;
  float: left;
  margin-right: 10px;
}
.hotsearch_cell .title text{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.hotsearch_cell .readnumber{
  font-size: 20rpx;
  color: #949494;
  float: left;
  margin-top:2px;
}
.hotsearch_cell .hotsearch_cell_right{
  width: 25px;
  height: 25px;
  line-height: 25px;
  border-radius: 10px;
  background: #fd2c6b;
  float: right;
  text-align: center;
  margin-top:20px;
  margin-right:22px;
  color:#fff;
  font-size:28rpx;
}