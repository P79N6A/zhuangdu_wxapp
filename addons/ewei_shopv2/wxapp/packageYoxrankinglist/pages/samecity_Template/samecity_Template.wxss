.samecity_cell{
  width:100%;
  height: 94px;
  background: #fff;
  border-bottom: 1px solid #e4e4e4;
}
.samecity_cell .image{
  width:66px;
  height: 66px;
  float: left;
  position: relative;
  margin-left:24px;
  margin-top:14px;
  margin-right:14px;
}
.samecity_cell .image image{
  background: #ccc;
  width:100%;
  height: 100%;
}
.samecity_cell .samecity_cell_left{
  float: left;
}
.samecity_cell .title{
  font-size: 30rpx;
  margin-top:26px;
}
.samecity_cell .title text{

}
.samecity_cell .readnumber{
  font-size: 20rpx;
  color: #cacaca;
  margin-top:8px;
}
.othericty{
  width:100%;
  height: 42px;
  line-height:42px;
}
.othericty text{
  font-size: 24rpx;
}
.othericty .samecity_icon{
  width:16px;
  height:16px;
  margin-right:14px;
}
.othericty .samecity_icon image{
  width: 100%;
  height: 100%;
}
.out{
  position: relative;
  width:22px;
  height: 22px;
  border-radius: 50%;
  border: 3px solid #fd2c6b;
  background: #fff;
  float:left;
  margin-left:24px;
  margin-top:7px;
  margin-right:10px;
}
.in{
  position: absolute;
  z-index: 5;
  width:11px;
  height: 11px;
  border-radius: 50%;
  border: 3px solid #fd2c6b;
  background-color: #fff;
  text-align: center;
  margin: auto;        
  left: 0;
  top: -2px;
  right: 0;
  bottom: 0;
}
.init{
  width:6px;
  height: 6px;
  background: #fd2c6b;
  border-radius: 6px;
  position: absolute;
  left: 36%;
  top: 3px;
}