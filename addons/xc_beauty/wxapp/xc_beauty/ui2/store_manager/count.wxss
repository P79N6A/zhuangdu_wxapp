page {
    background: #f7f7f7;
    padding-top: 10rpx;
    box-sizing: border-box;
}

.list {
    background: #fff;
    padding: 0 30rpx;
}

.list .title {
    display: flex;
    padding: 20rpx 0;
}

.list .title>view:nth-child(1) {
    width: 10rpx;
    height: 40rpx;
    background: #000;
    margin-right: 10rpx;
}

.list .item {
    display: flex;
    padding: 20rpx 0;
    border-top: #ddd 2rpx solid;
}

.list .item>view:nth-child(1) {
    flex: 1;
    color: #999;
}