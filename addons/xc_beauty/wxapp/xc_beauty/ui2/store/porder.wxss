page {
    background: #f7f7f7;
}

.nav {
    display: flex;
    background: #fff;
    border-top: #ddd 2rpx solid;
}

.nav .item {
    flex: 1;
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 20rpx 0;
    font-size: 4.5vw;
}

.nav .item.curr {
    background: #444;
    color: #fff;
}

.nav .item>image {
    margin-right: 10rpx;
}

.list {
    background: #fff;
    margin-top: 10rpx;
    padding: 0 30rpx;
}

.list .item {
    display: flex;
    padding: 20rpx 0;
    color: #999999;
}

.list .item>view:nth-child(2) {
    flex: 1;
    text-align: right;
    color: #000000;
}

.list .item+.item {
    border-top: #ddd 2rpx dashed;
}

.list .item .price {
    color: #999;
    text-decoration: line-through;
}

.list .item .num {
    display: flex;
    justify-content: flex-end;
}

.list .item .num>view:nth-child(1),.list .item .num>view:nth-child(3) {
    width: 50rpx;
    height: 50rpx;
    border-radius: 100%;
    color: #fff;
    background: #444;
    text-align: center;
    line-height: 50rpx;
}

.list .item .num>view:nth-child(2) {
    width: 60rpx;
    height: 50rpx;
    text-align: center;
    line-height: 50rpx;
}

.sign {
    padding: 30rpx 30rpx 0;
    background: #fff;
    margin-top: 10rpx;
}

.sign .content {
    padding: 0 20rpx;
}

.sign .content .item {
    display: flex;
    padding: 10rpx 0;
    align-items: center;
}

.sign .content .item>view:nth-child(1) {
    color: #999999;
}

.sign .content .item>input {
    flex: 1;
}

.sign .content .item>image {
    width: 40rpx;
}

.sign .content .item+.item {
    border-top: #ddd 2rpx solid;
}

.sign_btn {
    padding: 60rpx 30rpx;
    background: #fff;
    display: block;
}

.btn {
    height: 88rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #fff;
    border-radius: 10rpx;
}

.sign .content .date {
    display: flex;
    align-items: center;
    border-bottom: #ddd 2rpx solid;
}

.sign .content .date .date_l {
    color: #999999;
    font-size: 5vw;
    transform: rotateY(180deg);
}

.sign .content .date .date_r {
    color: #999999;
    font-size: 5vw;
}

.sign .content .date .date_item {
    text-align: center;
    line-height: 4vw;
    width: 120rpx;
    padding: 10rpx 0;
}

.sign .content .date .date_item>view:nth-child(1) {
    font-size: 4vw;
}

.sign .content .times {
    padding-top: 20rpx;
    overflow: hidden;
}

.sign .content .times>view {
    color: #999999;
    border: #999999 2rpx solid;
    border-radius: 10rpx;
    float: left;
    width: 200rpx;
    text-align: center;
    height: 50rpx;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-bottom: 20rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.sign .content .times>view text {
    width: 46rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.sign .content .times>view:nth-child(3n),.sign .content .times>view:nth-child(3n-1) {
    margin-left: 25rpx;
}

.store_page {
    background: #f7f7f7;
    position: fixed;
    top: 0;
    height: 100vh;
    overflow: scroll;
    width: 100%;
    left: 0;
    z-index: 101;
}

.store_page .item {
    background: #fff;
}

.store_page .item+.item {
    margin-top: 10rpx;
}

.store_page .item .item_t {
    display: flex;
    padding: 20rpx;
}

.store_page .item .item_t>image {
    width: 100rpx;
    height: 100rpx;
    border-radius: 100%;
}

.store_page .item .item_t .item_l {
    flex: 1;
    width: 450rpx;
    box-sizing: border-box;
    padding-left: 20rpx;
}

.store_page .item .item_t .item_l>view {
    width: 450rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #999;
    line-height: 4vw;
}

.store_page .item .item_t .item_l>view:nth-child(1) {
    color: #333;
    font-size: 4vw;
    margin-bottom: 10rpx;
}

.store_page .item .item_t .item_r {
    display: flex;
    align-items: center;
    flex-direction: column;
    justify-content: center;
    line-height: 4vw;
    color: #999;
    border-left: #f3a7bf 2rpx dashed;
    padding-left: 20rpx;
    width: 100rpx;
    box-sizing: border-box;
}

.store_page .item .item_b {
    display: flex;
}

.store_page .item .item_b>view {
    flex: 1;
    height: 70rpx;
    color: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
}

.store_page .item .item_b>view:nth-child(1) {
    background: #f3a7bf;
}

.store_page .item .item_b image {
    width: 30rpx;
    height: 30rpx;
    margin-right: 14rpx;
}

.member_page {
    background: #f7f7f7;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100vh;
    overflow: scroll;
    z-index: 101;
}

.member_page .member_list .item {
    background: #fff;
    display: flex;
    padding: 20rpx;
}

.member_page .member_list .item+.item {
    margin-top: 10rpx;
}

.member_page .member_list .item>image {
    width: 100rpx;
    height: 100rpx;
    border: #ddd 2rpx solid;
    border-radius: 100%;
}

.member_page .member_list .item .item_r {
    margin-left: 20rpx;
    flex: 1;
    width: 560rpx;
    padding: 10rpx 0;
    position: relative;
}

.member_page .member_list .item .item_r .name {
    font-size: 4vw;
}

.member_page .member_list .item .item_r .name .task {
    color: #B9B9B9;
    font-size: 3.5vw;
}

.member_page .member_list .item .item_r .tip {
    overflow: hidden;
    color: #B9B9B9;
    margin-top: 10rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 560rpx;
    height: 40rpx;
}

.member_page .member_list .item .item_r .tip>view {
    float: left;
}

.member_page .member_list .item .item_r .tip .tip_b {
    background: #ffbfbf;
    border-radius: 20rpx;
    padding: 0 20rpx;
    color: #fff;
}

.member_page .member_list .item .item_r .tip .tip_b+.tip_b {
    margin-left: 10rpx;
}

.member_page .member_list .item .item_r .zan {
    position: absolute;
    top: 10rpx;
    right: 0;
    display: flex;
    align-items: center;
    color: #b5b5b5;
    font-size: 4vw;
}

.member_page .member_list .item .item_r .zan>image {
    width: 30rpx;
    height: 30rpx;
    margin-right: 10rpx;
}

.service_page {
    position: fixed;
    bottom: 0;
    left: 0;
    background: #fff;
    width: 100%;
    height: 60vh;
    overflow: scroll;
    z-index: 1000;
}

.service_page .item {
    display: flex;
    padding: 20rpx;
    align-items: center;
    font-size: 4vw;
}

.service_page .item+.item {
    border-top: #ddd 2rpx solid;
}

.service_page .item>view:nth-child(1) {
    flex: 1;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.service_page .item>view:nth-child(2) {
    flex: 1;
    text-align: center;
}

.service_page .item>view:nth-child(3) {
    width: 120rpx;
    height: 50rpx;
    background: #e53770;
    color: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: 50rpx;
}

.return {
    position: fixed;
    bottom: 20vh;
    right: 20rpx;
    z-index: 100000000000000000;
}

.return>image {
    margin-bottom: 10rpx;
}