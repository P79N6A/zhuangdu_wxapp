page {
    background: #f7f7f7;
}

.page .top {
    padding: 30rpx;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 10;
    box-sizing: border-box;
}

.page .top .top_btn {
    background: #fff;
    text-align: center;
    line-height: 90rpx;
    font-size: 4vw;
}

.page .top .top_c {
    background: #fff;
    line-height: 90rpx;
    font-size: 4vw;
    display: flex;
    align-items: center;
    padding: 0 30rpx;
    box-sizing: border-box;
}

.top_block {
    height: 150rpx;
}

.page .top .top_c>image {
    width: 50rpx;
    height: 50rpx;
    border-radius: 100%;
}

.page .top .top_c>view:nth-child(2) {
    flex: 1;
    padding: 0 10rpx;
    width: 500rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.page .top .top_c .qie {
    width: 40rpx;
    height: 40rpx;
}

.list {
    overflow: hidden;
}

.list .item {
    width: 33.3333333333333%;
    box-sizing: border-box;
    padding: 30rpx;
    background: #fff;
    text-align: center;
    float: left;
    margin-bottom: 10rpx;
}

.list .item>image {
    width: 180rpx;
    height: 180rpx;
    box-sizing: border-box;
    margin: 0 auto;
    border-radius: 100%;
}

.list .item>view:nth-child(2) {
    margin-top: 20rpx;
    font-size: 4vw;
}

.list .item>view:nth-child(3) {
    color: #999;
}

.list .item .btn {
    width: 130rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    margin: 10rpx auto 0;
    color: #fff;
    border-radius: 40rpx;
    font-size: 4vw;
}

.store_page {
    background: #f7f7f7;
    position: fixed;
    top: 0;
    height: 100vh;
    overflow: scroll;
    width: 100%;
    left: 0;
    z-index: 100;
}

.store_page .item {
    background: #fff;
}

.store_page .item+.item {
    margin-top: 10rpx;
}

.store_page .item .item_t {
    display: flex;
    padding: 20rpx;
}

.store_page .item .item_t>image {
    width: 100rpx;
    height: 100rpx;
    border-radius: 100%;
}

.store_page .item .item_t .item_l {
    flex: 1;
    width: 450rpx;
    box-sizing: border-box;
    padding-left: 20rpx;
}

.store_page .item .item_t .item_l>view {
    width: 450rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #999;
    line-height: 4vw;
}

.store_page .item .item_t .item_l>view:nth-child(1) {
    color: #333;
    font-size: 4vw;
    margin-bottom: 10rpx;
}

.store_page .item .item_t .item_r {
    display: flex;
    align-items: center;
    flex-direction: column;
    justify-content: center;
    line-height: 4vw;
    color: #999;
    border-left: #f3a7bf 2rpx dashed;
    padding-left: 20rpx;
    width: 100rpx;
    box-sizing: border-box;
}

.store_page .item .item_b {
    display: flex;
}

.store_page .item .item_b>view {
    flex: 1;
    height: 70rpx;
    color: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
}

.store_page .item .item_b>view:nth-child(1) {
    background: #f3a7bf;
}

.store_page .item .item_b image {
    width: 30rpx;
    height: 30rpx;
    margin-right: 14rpx;
}

.return {
    position: fixed;
    bottom: 20vh;
    right: 20rpx;
    z-index: 100000000000000000;
}

.return>image {
    margin-bottom: 10rpx;
}