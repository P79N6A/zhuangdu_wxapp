<view class="wxf1">
    <view class="werow DiyNav DiyNavBrb">
        <view class="Navicon wecolumn" style="padding-left:5px;color:#000;">
            <view class="wxf1"></view>
            <i-icon bindtap="fanhui" size="20" style="line-height:4px;" type="fanhui"></i-icon>
            <view class="wxf1"></view>
        </view>
        <view class="wxf1 NavText">修改信息</view>
        <view class="Navicon"></view>
    </view>
    <view bindtap="chooseImage" class="Userimg werow">
        <view class="wxf1 ml20 wecolumn">
            <view class="wxf1"></view>
            <view>头像</view>
            <view class="wxf1"></view>
        </view>
        <view class=" mr20 tr wecolumn">
            <view class="wxf1"></view>
            <image class="ImageInfo" src="{{UserImages}}"></image>
            <view class="wxf1"></view>
        </view>
    </view>
    <view class="Name werow ">
        <view class="ml20 wecolumn">
            <view class="wxf1"></view>
            <view class="">昵称</view>
            <view class="wxf1"></view>
        </view>
        <input bindblur="NameData" class="wxf1 ml20 mr20" style="height:100%" value="{{Name}}"></input>
    </view>
    <button bindtap="Sub" class="circleCreateSub">保存</button>
</view>
<view class="actf" style="position: flex" wx:if="{{advshow==1}}">
    <view bindtap="Closeadvert" class="GGHide">关闭</view>
    <ad unitId="{{advCode}}"></ad>
</view>
<view style="height:9%"></view>
<view class="footerText">
  {{banquan}}
</view>
