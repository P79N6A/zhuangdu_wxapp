page {
    height: 100%;
}

.jinClick {
    position: absolute;
    width: 140px;
    height: 48px;
    left: 0;
    right: 0;
    margin: auto;
    bottom: 25%;
    z-index: 1;
    color: #fff;
    line-height: 48px;
    font-size: 16px;
    border-radius: 48px;
    background-image: linear-gradient(to bottom,#ff4081 5%,#c42659 100%);
}

.startShow {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
}

.huan {
    background-color: #ce4d44;
}

.footerText {
    position: fixed;
    left: 0;
    right: 0;
    bottom: 3%;
    text-align: center;
    font-size: 12px;
    color: rgb(119,118,118);
}